{
    "rewrites": [
      {
        "source": "/api/(.*)",
        "destination": "/server/dist/server.js"
      },
      {
        "source": "/(.*)",
        "destination": "/client/dist/index.html"
      }
    ],
    "redirects": [
      {
        "source": "/404",
        "destination": "/",
        "permanent": true
      }
    ]
  }
  
  